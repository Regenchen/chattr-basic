read_spchtbl <- function(spchtbl, sep, headerTF) {
sep <- ifelse(missing(sep), ',', sep)
headerTF <- ifelse(missing(headerTF), FALSE, headerTF)
spchdata <- read_delim(file = spchtbl, delim = sep, col_names = headerTF)
return(spchdata)
}
